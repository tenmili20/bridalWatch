<!DOCTYPE html>
<html lang="ja">
<head>
<meta charset="utf-8">
<title>WEDDING WATCH</title>
<meta name="description" content="ディスクリプションを入力">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" href="/css/styleSheet.css">
<link rel="stylesheet" href="/css/reset.css">
<link href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" rel="stylesheet">
<!-- [endif] -->
</head>
<body>
<!----- header----->
<header>
    <h1>Bridal Watch特集</h1>
    <div class="main-menu">
        <i class="fas fa-bars fa-2x"></i>
    </div>
</header>
<!----- /header ----->
<!----- main ----->
<div class="top-contents">
    <h1>Bridal Watch</h1>
    <h2>AAAAAAAAAAAAA</h2>
</div>
<!----- /main ----->
<div class="bottom-contents">
    <h2>ブランド一覧</h2>
    <div class="watches">
        <% list.forEach((item) => { %>
            <div class="watch">
               <img src="/img/SEIKO.jpg" alt="" />
               <div class="explain">
                   <p>ブランド名: <%= item.name %></p>
                   <p>平均価格帯: <%= item.price %></p>
                   <p>特徴: <%= item.feature %></p>
               </div>
               <div class="site">
                    <a href="" class="site-btn">公式サイト</a>
               </div>
           </div>
        <% }) %>
    </div>
    <div class="prevNext"> 
        <div class="prev">
            <p>前へ</p>
        </div>
        <div class="next">
            <p>次へ</p>
        </div>
    </div>
</div>
<!----- footer ----->
<footer>
    <h2>株式会社sample</h2>
    <ul>
        <li>A</li>
        <li>B</li>
        <li>C</li>
    </ul>
</footer>
<!----- /footer ----->
</body>
</html>